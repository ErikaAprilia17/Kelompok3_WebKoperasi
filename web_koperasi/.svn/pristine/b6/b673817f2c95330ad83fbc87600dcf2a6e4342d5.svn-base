<?php
class data_anggota extends CI_Controller {

	public function index()
	{
		$data = array(
			'title' => 'Data Anggota',
			'page' => 'admin/laporan/v_data_anggota',
			'data_anggota' => $this->db->get('data_anggota')->result(),
			
		);
		$this->load->view('admin/layout/template',$data);
		}
		 public function cetak(){
    ob_start();
    $data['data_anggota'] = $this->db->get('data_anggota')->result();
    $this->load->view('admin/laporan/print', $data);
    $html = ob_get_contents();
        ob_end_clean();
        
        require_once('./assets/html2pdf/html2pdf.class.php');
    $pdf = new HTML2PDF('P','A4','en');
    $pdf->WriteHTML($html);
    $pdf->Output('Data Anggota.pdf', 'D');
  }

	}